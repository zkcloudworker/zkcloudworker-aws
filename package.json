{
  "name": "zkcloudworker-aws",
  "description": "zkCloudWorker AWS",
  "keywords": [
    "zkCloudWorker",
    "backend",
    "cloud",
    "proof",
    "mina",
    "zkapp",
    "zk",
    "blockchain",
    "web3",
    "zero knowledge"
  ],
  "version": "0.5.0",
  "author": "DFST",
  "license": "Apache-2.0",
  "scripts": {
    "deploy": "serverless deploy --aws-profile zkcloudworker --use-local-credentials",
    "prune": "serverless prune --aws-profile zkcloudworker -n 5",
    "compile": "serverless package --aws-profile zkcloudworker",
    "info": "serverless info --aws-profile zkcloudworker",
    "org": "serverless --org=dfstio --aws-profile zkcloudworker",
    "profile": "serverless config credentials --provider aws --key ACCESS_KEY --secret SECRET_KEY --profile zkcloudworker --overwrite "
  },
  "devDependencies": {
    "@digitalmaas/serverless-plugin-lambda-dead-letter": "^1.0.0",
    "@types/adm-zip": "^0.5.6",
    "@types/aws-lambda": "^8.10.146",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.0",
    "@types/serverless": "^3.12.23",
    "serverless": "^3.39.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-prune-plugin": "^2.1.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.699.0",
    "@aws-sdk/client-dynamodb": "^3.699.0",
    "@aws-sdk/client-kms": "^3.699.0",
    "@aws-sdk/client-lambda": "^3.699.0",
    "@aws-sdk/client-lightsail": "^3.699.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "@aws-sdk/util-dynamodb": "^3.699.0",
    "@nats-io/jetstream": "^3.0.0-32",
    "@nats-io/kv": "^3.0.0-26",
    "@nats-io/transport-node": "^3.0.0-30",
    "adm-zip": "^0.5.16",
    "algoliasearch": "^4.24.0",
    "axios": "^1.7.8",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "rate-limiter-flexible": "^5.0.4"
  },
  "packageManager": "yarn@4.5.3"
}

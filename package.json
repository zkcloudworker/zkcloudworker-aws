{
  "name": "zkcloudworker-aws",
  "description": "zkCloudWorker AWS",
  "keywords": [
    "zkCloudWorker",
    "backend",
    "cloud",
    "proof",
    "mina",
    "zkapp",
    "zk",
    "blockchain",
    "web3",
    "zero knowledge"
  ],
  "version": "0.4.0",
  "author": "DFST",
  "license": "Apache-2.0",
  "scripts": {
    "deploy": "serverless deploy --aws-profile zkcloudworker --use-local-credentials",
    "prune": "serverless prune --aws-profile zkcloudworker -n 5",
    "compile": "serverless package --aws-profile zkcloudworker",
    "info": "serverless info --aws-profile zkcloudworker",
    "org": "serverless --org=dfstio --aws-profile zkcloudworker",
    "profile": "serverless config credentials --provider aws --key ACCESS_KEY --secret SECRET_KEY --profile zkcloudworker --overwrite "
  },
  "devDependencies": {
    "@digitalmaas/serverless-plugin-lambda-dead-letter": "^1.0.0",
    "@types/adm-zip": "^0.5.5",
    "@types/aws-lambda": "^8.10.140",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.10",
    "@types/serverless": "^3.12.22",
    "serverless": "^3.39.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-prune-plugin": "^2.0.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.609.0",
    "@aws-sdk/client-dynamodb": "^3.609.0",
    "@aws-sdk/client-kms": "^3.609.0",
    "@aws-sdk/client-lambda": "^3.609.0",
    "@aws-sdk/client-lightsail": "^3.609.0",
    "@aws-sdk/client-s3": "^3.609.0",
    "@aws-sdk/s3-request-presigner": "^3.609.0",
    "@aws-sdk/util-dynamodb": "^3.609.0",
    "adm-zip": "^0.5.14",
    "algoliasearch": "^4.24.0",
    "axios": "^1.7.2",
    "jsonwebtoken": "^9.0.2",
    "nats": "^2.28.0",
    "rate-limiter-flexible": "^5.0.3"
  },
  "packageManager": "yarn@4.2.2"
}
